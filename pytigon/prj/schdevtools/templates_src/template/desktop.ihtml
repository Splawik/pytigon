

%extends 'template/desktop_base.html'|translate:lang


% load exfiltry
% load exsyntax
% load bootstrap4

%% js_extrascipts
    {{block.super}}
    {% jscript_link 'vue/vue.min.js' %}{% jscript_link 'vue/httpVueLoader.js' %}
    
    {% module_link 'schdevtools/js/test_javascript.js' %}
    {% module_link 'schdevtools/js/test_python.js' %}
    
    

%% extrastyle
    {{block.super}}
    {% css_link 'schdevtools/css/style.css' %}
    {% css_link 'schdevtools/css/test_icss.css' %}
    
    

%% body_footer_scripts
    {{block.super}}
    
    .<script language='javascript'>var global_vue_bus = new Vue ();</script>
    {% component 'schcomponents/components/pivottable.js' %}
    {% component 'schcomponents/components/video-js.js' %}
    {% component 'schcomponents/components/xterm.js' %}
    {% component 'schcomponents/components/txtarea_editor.js' %}
    {% component 'schcomponents/components/handsontable.js' %}
    {% component 'schcomponents/components/plotly.js' %}
    {% component 'schcomponents/components/leaflet.js' %}
    {% component 'schcomponents/components/d3.js' %}
    {% component 'schdevtools/components/test.js' %}
    {% component 'schcomponents/components/vs_editor.js' %}
    {% component 'schcomponents/components/wysiwyg_edit.js' %}
    {% component 'schcomponents/components/summernote.js' %}
    
    

%% component_init
    .var component_init = [ "pivottable.js", "video-js.js", "xterm.js", "txtarea_editor.js", "handsontable.js", "plotly.js", "leaflet.js", "d3.js", "test.js", "vs_editor.js", "wysiwyg_edit.js", "summernote.js",  ];  

%% botstrap_css
    % comment:
        {% css_link 'themes/bootswatch/materia/bootstrap.min.css' %}

    {% css_link 'themes/bmd/css/bootstrap-material-design.min.css' %}


%% bootstrap_js
    % comment:
        {{ block.super }}
    {% jscript_link 'themes/bmd/js/bootstrap-material-design.min.js' %}

%% js_app_init
    .let db_struct = [ ["devparam", {"keyPath": "key"}] ];
    .init_db(db_struct);
    .const devtools_sync = function(run_after) {
    .    console.log("SYNC!");
    .    run_after("OK");
    .}
    .let sync_struct = [ ['devparam', "{{base_path}}schdevtools/devparam/", devtools_sync], ];
    .init_sync(sync_struct);

    .function bootstrap_init(elem) {
    .    elem.bootstrapMaterialDesign();
    .};

    .register_fragment_init_fun(bootstrap_init);

    {{ block.super }}
    

%% logo
    span class=logo-lg
        img src={{base_path}}static/favicon.ico,,,style=height:100%;
        span class=txt3d text-white,,,style=font-size:1rem;
            .Pytigon - dev tools
