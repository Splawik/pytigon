from django.utils.translation import ugettext_lazy as _

ModuleTitle = _('{{app.module_title}}')
Title = _('{{app.title}}')
Perms = {{app.perms|safe}}
Index = '{{app.index}}'
Urls  = ({% for item in app.schappmenu_set.all %}
    ('{{item.clean_url}}{{item.get_url_type_ext}}',_('{{item.name}}'),{{item.get_perms|safe}},'{{item.icon|safe}}{% if item.not_standard_icon_size %}?size={{item.icon_size}}{% endif %}'),{% endfor %}
)
UserParam = {{user_param|safe}}