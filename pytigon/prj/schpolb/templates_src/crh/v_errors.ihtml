{# -*- coding: utf-8 -*- #}
% extends "forms/user_tab.html"

% load exfiltry
% load exsyntax


%% all
    %with show_title_bar=1 title='Błędy w AD'
        {{ block.super }}


%% user_header
    th...company
    th...sAMAccountName
    th...mail
    th...upn
    th...errors


%% user_row
