    padding:0

td.menu_txt
    font-size:110%

body.toolbar
    border: 0
    padding: 0
    margins: 0 3 0 3

table.toolbar
    cellpadding:0
    cellspacing:0

table.toolbar tr td
    valign: top

ul.errorlist
    width:16
    height:16
    bgcolor: #f00

pre
    white-space: pre

pre span
    white-space: pre

table.codehilitetable
    border:1
    width:100%

td.linenos
    cellpadding:10
    border:0
    vertical-align:top
    valign: top
